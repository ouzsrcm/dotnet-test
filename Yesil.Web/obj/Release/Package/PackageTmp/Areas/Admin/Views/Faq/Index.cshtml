@model IEnumerable<Yesil.Data.Faq>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h3>Sıkça Sorulan Sorular</h3>

<p>
    @Html.ActionLink("Yeni ekle", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Soru
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Question)
        </td>
        <td>
            @Html.ActionLink("Düzenle", "Edit", new { id=item.FaqId }) |
            @Html.ActionLink("Sil", "Delete", new { id=item.FaqId })
        </td>
    </tr>
}
</table>
